<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.himedia.springboot.P_membercontrolDAO">
<select id="getmemberlist" resultType="com.himedia.springboot.P_membercontrolDTO">
	SELECT userid,NAME,gender,birthday,mobile,created,admin FROM MEMBER
</select>
<delete id="memberdelete">
	DELETE FROM MEMBER WHERE userid= #{param1};
</delete>
</mapper>